@startuml
'https://plantuml.com/class-diagram

class Student {
    - name: String
    - grade : GRADE
    - group : GROUP
    - secretNickname : String
    + Student(name: String, grade: GRADE, group : GROUP) : void
    + getGrade() : GRADE
    + getName(): String
    + increaseGrade(): void
    + decreaseGrade(): void
}
Student "1" -- "1" GROUP : has
Student "1" -- "1" GRADE : has

enum GROUP {
1
2
3
4
5
}

enum GRADE {
A
B
C
D
E
F
}

@enduml